    "@babel/core": "^7.2.2",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.2.3",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "jest": "^23.6.0",
    "knex": "^0.16.3",
    "supertest": "^3.4.1"
  },
  "dependencies": {
    "async-redis": "^1.1.5",
    "bcrypt": "^3.0.3",
    "body-parser": "^1.18.3",
    "express": "^4.16.4",
    "express-cache-controller": "^1.1.0",
    "express-request-id": "^1.4.1",
    "express-validator": "^5.3.1",
    "get-port": "^4.1.0",
    "helmet": "^3.15.0",
    "jsonwebtoken": "^8.4.0",
    "pg": "^7.8.0",
