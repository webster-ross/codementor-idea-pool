{
  "name": "idea-pool",
  "version": "1.0.0",
  "scripts": {
    "dev": "nodemon --exec babel-node src/index",
    "test": "knex migrate:latest && jest",
    "test:watch": "npm run test -- --watchAll",
    "knex": "knex",
    "migrate:latest": "knex migrate:latest",
    "migrate:rollback": "knex migrate:rollback",
